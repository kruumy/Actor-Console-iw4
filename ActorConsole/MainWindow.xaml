<Window x:Class="ActorConsole.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:tk="http://schemas.xceed.com/wpf/xaml/toolkit"
        xmlns:ui="http://schemas.modernwpf.com/2019"
        ui:WindowHelper.UseModernWindowStyle="True"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ActorConsole"
        mc:Ignorable="d"
        Title="IW4 Actor Console" MinHeight="750" MinWidth="825" Height="750" Width="825" Loaded="Window_Loaded" Closing="Window_Closing">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="22*"/>
            <ColumnDefinition Width="206*"/>
        </Grid.ColumnDefinitions>
        <TabControl x:Name="mainTabs" TabStripPlacement="Left"  Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="2">
            <TabItem Height="60" Width="70">
                <TabItem.Header>
                    <StackPanel Orientation="Horizontal">
                        <Image Source="Resources/home.png" Width="20" Height="20" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="10,0,0,0" />
                    </StackPanel>
                </TabItem.Header>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="20"></ColumnDefinition>
                        <ColumnDefinition Width="*"></ColumnDefinition>
                        <ColumnDefinition Width="*"></ColumnDefinition>
                        <ColumnDefinition Width="20"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="20"></RowDefinition>
                        <RowDefinition Height="*"></RowDefinition>
                        <RowDefinition Height="35"></RowDefinition>
                    </Grid.RowDefinitions>
                    <TextBlock x:Name="versionText" Grid.Row="1" Grid.Column="1" VerticalAlignment="Bottom" HorizontalAlignment="Left" Opacity="0.5" Margin="-16,0,0,-15" Text="v1.0.0" />
                    <TextBlock Grid.Row="1" Grid.ColumnSpan="2" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Top" FontSize="48" Margin="0,10,0,0" >Hello!</TextBlock>
                    <Button x:Name="precacheBtn" Grid.ColumnSpan="2" Grid.Row="1" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Top" FontSize="14" Margin="0,80,0,0" Click="precacheBtn_Click">Select Your Precache</Button>
                    <Image Width="200" Grid.Column="2" Grid.Row="1" Source="Resources/mvm.png" HorizontalAlignment="Right" VerticalAlignment="Bottom" Stretch="Uniform" Opacity="0.25" RenderTransformOrigin="0.5,0.5">
                        <Image.RenderTransform>
                            <TransformGroup>
                                <ScaleTransform/>
                                <SkewTransform/>
                                <RotateTransform/>
                                <TranslateTransform X="50"/>
                            </TransformGroup>
                        </Image.RenderTransform>
                    </Image>
                    <Image Width="90" Grid.Column="2" Grid.Row="1" Source="Resources/mg.png" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,0,95,0" RenderTransformOrigin="0.5,0.5" Opacity="0.25">
                        <Image.RenderTransform>
                            <TransformGroup>
                                <ScaleTransform/>
                                <SkewTransform/>
                                <RotateTransform/>
                                <TranslateTransform X="0" Y="10"/>
                            </TransformGroup>
                        </Image.RenderTransform>
                    </Image>
                </Grid>
            </TabItem>
            <TabItem x:Name="modelsItem" Height="60" MouseEnter="modelsItem_MouseEnter" IsEnabled="False">
                <TabItem.Header>
                    <StackPanel Orientation="Horizontal">
                        <Image Source="Resources/Models.png" Width="22" Height="22" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="10,0,0,0" />
                    </StackPanel>
                </TabItem.Header>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="5"></ColumnDefinition>
                        <ColumnDefinition Width="*"></ColumnDefinition>
                        <ColumnDefinition Width="*"></ColumnDefinition>
                        <ColumnDefinition Width="5"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="5"></RowDefinition>
                        <RowDefinition Height="*"></RowDefinition>
                        <RowDefinition Height="*"></RowDefinition>
                        <RowDefinition Height="25"></RowDefinition>
                    </Grid.RowDefinitions>
                    <Rectangle Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="4" Grid.RowSpan="2" Fill="#FF2B2B2B" VerticalAlignment="Top" Height="60" Margin="0,8,0,0"></Rectangle>
                    <Rectangle Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="3" Grid.RowSpan="2" Fill="#FF2B2B2B" VerticalAlignment="Center" Height="180" Margin="0,15,362,0"></Rectangle>
                    <ListBox x:Name="headBox" Grid.Row="2" Grid.ColumnSpan="1" Grid.RowSpan="1" Grid.Column="1" d:ItemsSource="{d:SampleData ItemCount=5}" HorizontalAlignment="Stretch" Margin="5,20,5,5" MouseDoubleClick="headBox_MouseDoubleClick"></ListBox>
                    <ListBox x:Name="bodyBox" Grid.Row="2" Grid.Column="2" d:ItemsSource="{d:SampleData ItemCount=5}" HorizontalAlignment="Stretch" Margin="5,20,5,5" MouseDoubleClick="bodyBox_MouseDoubleClick"></ListBox>
                    <TextBlock Grid.Row="2" Grid.Column="1" VerticalAlignment="Top" HorizontalAlignment="Center" Margin="0,0,1,0">Head Models</TextBlock>
                    <TextBlock Grid.Row="2" Grid.Column="2" VerticalAlignment="Top" HorizontalAlignment="Center">Body Models</TextBlock>
                    <TextBlock Grid.Row="1" Grid.Column="2" VerticalAlignment="Top" HorizontalAlignment="Center" Margin="0,55,0,0">SinglePlayer Models</TextBlock>
                    <ListBox x:Name="spBox" Grid.Row="1" Grid.Column="2" d:ItemsSource="{d:SampleData ItemCount=5}" HorizontalAlignment="Stretch" Margin="5,75,5,5" MouseDoubleClick="spBox_MouseDoubleClick"></ListBox>

                    <TextBlock Grid.Column="1" Grid.Row="1" Grid.ColumnSpan="2" VerticalAlignment="Top" HorizontalAlignment="Center" Margin="10,0,0,0" Text="Current Map" />
                    <TextBox x:Name="mapBox" Grid.Column="1" Grid.Row="1" FontSize="15" VerticalAlignment="Top" Margin="5,20,5,0" KeyUp="mapBox_KeyUp" Grid.ColumnSpan="2"/>
                    <TextBlock Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Center" Text="Selected Head Model" Grid.Column="1" Margin="0,0,0,130" />
                    <TextBlock Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Center" Text="Selected Body Model" Grid.Column="1" Margin="0,0,0,10" />
                    <TextBox x:Name="selectedHead" Grid.Column="1" Grid.Row="1" FontSize="15" VerticalAlignment="Center" Margin="5,0,5,70"/>
                    <TextBox x:Name="selectedBody" Grid.Column="1" Grid.Row="1" FontSize="15" VerticalAlignment="Center" Margin="5,50,5,0"/>
                    <Button x:Name="sendModelsBtn" Grid.Column="1" Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center" IsEnabled="False" Margin="0,135,0,0" Click="sendModelsBtn_Click">Send Models</Button>
                    <ui:ToggleSwitch x:Name="spModelSwitch" OnContent="Head" OffContent="Body" Grid.Row="1" Grid.Column="2" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,60,-30,10">
                        <ui:ToggleSwitch.Effect>
                            <DropShadowEffect BlurRadius="30" ShadowDepth="0" Opacity="30" Color="DarkGray"/>
                        </ui:ToggleSwitch.Effect>
                    </ui:ToggleSwitch>



                </Grid>
            </TabItem>
            <TabItem x:Name="animsItem" Height="60" MouseEnter="animsItem_MouseEnter" IsEnabled="False">
                <TabItem.Header>
                    <StackPanel Orientation="Horizontal">
                        <Image Source="Resources/anims.png" Width="22" Height="22" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="10,0,0,0" />
                    </StackPanel>
                </TabItem.Header>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="5"></ColumnDefinition>
                        <ColumnDefinition Width="*"></ColumnDefinition>
                        <ColumnDefinition Width="*"></ColumnDefinition>
                        <ColumnDefinition Width="5"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="5"></RowDefinition>
                        <RowDefinition Height="*"></RowDefinition>
                        <RowDefinition Height="*"></RowDefinition>
                        <RowDefinition Height="25"></RowDefinition>
                    </Grid.RowDefinitions>

                    <ListBox x:Name="idleAnimBox" Grid.Row="2" Grid.Column="1" d:ItemsSource="{d:SampleData ItemCount=5}" HorizontalAlignment="Stretch" Margin="5,20,5,5" MouseDoubleClick="idleAnimBox_MouseDoubleClick"/>
                    <ListBox x:Name="deathAnimBox" Grid.Row="2" Grid.Column="2" d:ItemsSource="{d:SampleData ItemCount=5}" HorizontalAlignment="Stretch" Margin="5,20,5,4" MouseDoubleClick="deathAnimBox_MouseDoubleClick"/>
                    <ListBox x:Name="spAnimBox" Grid.Row="1" Grid.Column="2" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="5,20,5,5" HorizontalAlignment="Stretch" MouseDoubleClick="spAnimBox_MouseDoubleClick" VerticalAlignment="Stretch"></ListBox>
                    <TextBlock Grid.Row="2" Grid.Column="1" VerticalAlignment="Top" HorizontalAlignment="Center">Idle Anims</TextBlock>
                    <TextBlock Grid.Row="2" Grid.Column="2" VerticalAlignment="Top" HorizontalAlignment="Center">Death Anims</TextBlock>
                    <TextBlock Grid.Row="1" Grid.Column="2" VerticalAlignment="Top" HorizontalAlignment="Center">SinglePlayer Anims</TextBlock>
                    <ui:ToggleSwitch x:Name="spAnimSwitch" OnContent="Idle" OffContent="Death" Grid.Row="1" Grid.Column="2" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,60,-30,10">
                        <ui:ToggleSwitch.Effect>
                            <DropShadowEffect BlurRadius="30" ShadowDepth="0" Opacity="30" Color="DarkGray"/>
                        </ui:ToggleSwitch.Effect>
                    </ui:ToggleSwitch>
                    <Rectangle Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="3" Grid.RowSpan="2" Fill="#FF2B2B2B" VerticalAlignment="Center" Height="180" Margin="0,5,362,0"></Rectangle>
                    <TextBlock Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Center" Grid.Column="1" Margin="0,0,0,140" >Selected Idle Anim</TextBlock>
                    <TextBlock Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Center" Grid.Column="1" Margin="0,0,0,20" >Selected Death Anim</TextBlock>
                    <TextBox x:Name="currentIdleAnimBox" Grid.Column="1" Grid.Row="1" FontSize="15" VerticalAlignment="Center" Margin="5,0,5,80"/>
                    <TextBox x:Name="currentDeathAnimBox" Grid.Column="1" Grid.Row="1" FontSize="15" VerticalAlignment="Center" Margin="5,40,5,0"/>
                    <Button x:Name="sendAnimsBtn" Grid.Column="1" Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center" IsEnabled="False" Margin="0,125,0,0" Click="sendAnimsBtn_Click">Apply Anims</Button>



                </Grid>
            </TabItem>
            <TabItem x:Name="weaponsItem" Height="60" IsEnabled="False">
                <TabItem.Header>
                    <StackPanel Orientation="Horizontal">
                        <Image Source="Resources/gun.png" Width="22" Height="22" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="10,0,0,0" />
                    </StackPanel>
                </TabItem.Header>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="0"></ColumnDefinition>
                        <ColumnDefinition Width="*"></ColumnDefinition>
                        <ColumnDefinition Width="0"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="0"></RowDefinition>
                        <RowDefinition Height="*"></RowDefinition>
                        <RowDefinition Height="20"></RowDefinition>
                    </Grid.RowDefinitions>
                    <ComboBox x:Name="ComboWeaponBone" Grid.Column="1" Grid.Row="1" Panel.ZIndex="5" VerticalAlignment="Bottom" HorizontalAlignment="Left" Margin="10,0,0,45" Width="114.756666666667" FontSize="12" SelectedIndex="0" Height="28" >
                        <ComboBoxItem Content="j_gun"/>
                        <ComboBoxItem Content="tag_weapon_right"/>
                        <ComboBoxItem Content="tag_weapon_left"/>
                        <ComboBoxItem Content="tag_stowed_back"/>
                        <ComboBoxItem Content="tag_stowed_hip_rear"/>
                        <ComboBoxItem Content="tag_inhand"/>
                    </ComboBox>
                    <TextBlock Grid.Row="1" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,125" Opacity="0.5">Attachments Coming Soon</TextBlock>
                    <Button x:Name="sendWeaponsBtn" Grid.Column="1" Grid.Row="1" Panel.ZIndex="5" VerticalAlignment="Bottom" HorizontalAlignment="Left" Margin="10,0,0,10" Click="sendWeaponsBtn_Click" IsEnabled="False">Apply Weapons</Button>
                    <Button x:Name="removeWeaponsBtn" Grid.Column="1" Grid.Row="1" Panel.ZIndex="5" VerticalAlignment="Bottom" HorizontalAlignment="Left" Margin="133,0,0,10" Click="removeWeaponsBtn_Click" IsEnabled="False">Remove Weapon</Button>
                    <TabControl Grid.Row="1" Grid.Column="1">
                        <TabItem Header="Assault Rifles">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="1"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="10"></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="10"></RowDefinition>
                                    <RowDefinition Height="*"></RowDefinition>
                                    <RowDefinition Height="*"></RowDefinition>
                                    <RowDefinition Height="*"></RowDefinition>
                                    <RowDefinition Height="*"></RowDefinition>
                                    <RowDefinition Height="10"></RowDefinition>
                                </Grid.RowDefinitions>
                                <Rectangle Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="7" Grid.RowSpan="1" Height="5" Fill="#FF999999" Width="auto" VerticalAlignment="Top" Margin="0,20,0,0"/>
                                <Image Grid.Column="1" Grid.Row="1" Height="125" Width="125" Source="Resources/M4A1-00001.png"/>
                                <CheckBox x:Name="M4A1check" Grid.Row="1" Grid.Column="1" VerticalAlignment="Bottom" HorizontalAlignment="Center" Checked="M4A1check_Checked" >M4A1</CheckBox>
                                <Image Grid.Column="2" Grid.Row="1" Height="125" Width="125" Source="Resources/FAMAS-00001.png"/>
                                <CheckBox x:Name="FAMAScheck" Grid.Row="1" Grid.Column="2" VerticalAlignment="Bottom" HorizontalAlignment="Center" Checked="FAMAScheck_Checked" >FAMAS</CheckBox>
                                <Image Grid.Column="3" Grid.Row="1" Height="125" Width="125" Source="Resources/SCARH-00001.png"/>
                                <CheckBox x:Name="SCARHcheck" Grid.Row="1" Grid.Column="3" VerticalAlignment="Bottom" HorizontalAlignment="Center" Checked="SCARHcheck_Checked" >SCAR-H</CheckBox>
                                <Image Grid.Column="4" Grid.Row="1" Height="125" Width="125" Source="Resources/TAR21-00001.png"/>
                                <CheckBox x:Name="TAR21check" Grid.Row="1" Grid.Column="4" VerticalAlignment="Bottom" HorizontalAlignment="Center" Checked="TAR21check_Checked" >TAR-21</CheckBox>
                                <Image Grid.Column="1" Grid.Row="2" Height="125" Width="125" Source="Resources/FAL-00001.png"/>
                                <CheckBox x:Name="FALcheck" Grid.Row="2" Grid.Column="1" VerticalAlignment="Bottom" HorizontalAlignment="Center" Checked="FALcheck_Checked" >FAL</CheckBox>
                                <Image Grid.Column="2" Grid.Row="2" Height="125" Width="125" Source="Resources/M16A4-00001.png"/>
                                <CheckBox x:Name="M16A4check" Grid.Row="2" Grid.Column="2" VerticalAlignment="Bottom" HorizontalAlignment="Center" Checked="M16A4check_Checked" >M16A4</CheckBox>
                                <Image Grid.Column="3" Grid.Row="2" Height="125" Width="125" Source="Resources/ACR-00001.png"/>
                                <CheckBox x:Name="ACRcheck" Grid.Row="2" Grid.Column="3" VerticalAlignment="Bottom" HorizontalAlignment="Center" Checked="ACRcheck_Checked" >ACR</CheckBox>
                                <Image Grid.Column="4" Grid.Row="2" Height="125" Width="125" Source="Resources/F2000-00001.png"/>
                                <CheckBox x:Name="F2000check" Grid.Row="2" Grid.Column="4" VerticalAlignment="Bottom" HorizontalAlignment="Center" Checked="F2000check_Checked" >F2000</CheckBox>
                                <Image Grid.Column="5" Grid.Row="1" Height="125" Width="125" Source="Resources/AK47-00001.png"/>
                                <CheckBox x:Name="AK47check" Grid.Row="1" Grid.Column="5" VerticalAlignment="Bottom" HorizontalAlignment="Center" Checked="AK47check_Checked" >AK-47</CheckBox>


                            </Grid>

                        </TabItem>
                        <TabItem Header="Sub Machine Guns">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="1"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="10"></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="10"></RowDefinition>
                                    <RowDefinition Height="*"></RowDefinition>
                                    <RowDefinition Height="*"></RowDefinition>
                                    <RowDefinition Height="*"></RowDefinition>
                                    <RowDefinition Height="*"></RowDefinition>
                                    <RowDefinition Height="10"></RowDefinition>
                                </Grid.RowDefinitions>
                                <Rectangle Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="7" Grid.RowSpan="1" Height="5" Fill="#FFE6E6E6" Width="auto" VerticalAlignment="Top" Margin="0,20,0,0"/>
                                <Image Grid.Column="1" Grid.Row="1" Height="125" Width="125" Source="Resources/MP5K-00001.png"/>
                                <CheckBox x:Name="MP5Kcheck" Grid.Row="1" Grid.Column="1" VerticalAlignment="Bottom" HorizontalAlignment="Center" Checked="MP5Kcheck_Checked" >MP5k</CheckBox>
                                <Image Grid.Column="2" Grid.Row="1" Height="125" Width="125" Source="Resources/UMP45-00001.png"/>
                                <CheckBox x:Name="UMP45check" Grid.Row="1" Grid.Column="2" VerticalAlignment="Bottom" HorizontalAlignment="Center" Checked="UMP45check_Checked" >UMP45</CheckBox>
                                <Image Grid.Column="3" Grid.Row="1" Height="125" Width="125" Source="Resources/Vector-00001.png"/>
                                <CheckBox x:Name="Vectorcheck" Grid.Row="1" Grid.Column="3" VerticalAlignment="Bottom" HorizontalAlignment="Center" Checked="Vectorcheck_Checked" >Vector</CheckBox>
                                <Image Grid.Column="4" Grid.Row="1" Height="125" Width="125" Source="Resources/P90-00001.png"/>
                                <CheckBox x:Name="P90check" Grid.Row="1" Grid.Column="4" VerticalAlignment="Bottom" HorizontalAlignment="Center" Checked="P90check_Checked" >P90</CheckBox>
                                <Image Grid.Column="5" Grid.Row="1" Height="125" Width="125" Source="Resources/MiniUzi-00001.png"/>
                                <CheckBox x:Name="MiniUzicheck" Grid.Row="1" Grid.Column="5" VerticalAlignment="Bottom" HorizontalAlignment="Center" Checked="MiniUzicheck_Checked" >Mini-Uzi</CheckBox>
                                <Image Grid.Column="1" Grid.Row="2" Height="125" Width="125" Source="Resources/AK74u-00001.png"/>
                                <CheckBox x:Name="AK47Ucheck" Grid.Row="2" Grid.Column="1" VerticalAlignment="Bottom" HorizontalAlignment="Center" Checked="AK47Ucheck_Checked" >AK-74u</CheckBox>
                                <Image Grid.Column="2" Grid.Row="2" Height="125" Width="125" Source="Resources/Peacekeeper-00001.png"/>
                                <CheckBox x:Name="Peacekeepercheck" Grid.Row="2" Grid.Column="2" VerticalAlignment="Bottom" HorizontalAlignment="Center" Checked="Peacekeepercheck_Checked" >Peacekeeper</CheckBox>
                                <TextBlock Grid.Column="6" Grid.Row="1" Grid.RowSpan="4" Grid.ColumnSpan="2" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="#FF676464">Attachments Coming Soon</TextBlock>

                            </Grid>
                        </TabItem>
                        <TabItem Header="Light Machine Guns">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="1"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="10"></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="10"></RowDefinition>
                                    <RowDefinition Height="*"></RowDefinition>
                                    <RowDefinition Height="*"></RowDefinition>
                                    <RowDefinition Height="*"></RowDefinition>
                                    <RowDefinition Height="*"></RowDefinition>
                                    <RowDefinition Height="10"></RowDefinition>
                                </Grid.RowDefinitions>
                                <Rectangle Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="7" Grid.RowSpan="1" Height="5" Fill="#FFE6E6E6" Width="auto" VerticalAlignment="Top" Margin="0,20,0,0"/>
                                <Image Grid.Column="1" Grid.Row="1" Height="125" Width="125" Source="Resources/L86LSW-00001.png"/>
                                <CheckBox x:Name="L86LSWcheck" Grid.Row="1" Grid.Column="1" VerticalAlignment="Bottom" HorizontalAlignment="Center" Checked="L86LSWcheck_Checked" >L86 LSW</CheckBox>
                                <Image Grid.Column="2" Grid.Row="1" Height="125" Width="125" Source="Resources/RPD-00001.png"/>
                                <CheckBox x:Name="RPDcheck" Grid.Row="1" Grid.Column="2" VerticalAlignment="Bottom" HorizontalAlignment="Center" Checked="RPDcheck_Checked" >RPD</CheckBox>
                                <Image Grid.Column="3" Grid.Row="1" Height="125" Width="125" Source="Resources/MG4-00001.png"/>
                                <CheckBox x:Name="MG4check" Grid.Row="1" Grid.Column="3" VerticalAlignment="Bottom" HorizontalAlignment="Center" Checked="MG4check_Checked" >MG4</CheckBox>
                                <Image Grid.Column="4" Grid.Row="1" Height="125" Width="125" Source="Resources/AUG-00001.png"/>
                                <CheckBox x:Name="AUGcheck" Grid.Row="1" Grid.Column="4" VerticalAlignment="Bottom" HorizontalAlignment="Center" Checked="AUGcheck_Checked" >AUG HBAR</CheckBox>
                                <Image Grid.Column="5" Grid.Row="1" Height="125" Width="125" Source="Resources/M240-00001.png"/>
                                <CheckBox x:Name="M240check" Grid.Row="1" Grid.Column="5" VerticalAlignment="Bottom" HorizontalAlignment="Center" Checked="M240check_Checked" >M240</CheckBox>
                                <TextBlock Grid.Column="6" Grid.Row="1" Grid.RowSpan="4" Grid.ColumnSpan="2" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="#FF676464">Attachments Coming Soon</TextBlock>

                            </Grid>
                        </TabItem>
                        <TabItem Header="Sniper Rifles">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="1"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="10"></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="10"></RowDefinition>
                                    <RowDefinition Height="*"></RowDefinition>
                                    <RowDefinition Height="*"></RowDefinition>
                                    <RowDefinition Height="*"></RowDefinition>
                                    <RowDefinition Height="*"></RowDefinition>
                                    <RowDefinition Height="10"></RowDefinition>
                                </Grid.RowDefinitions>
                                <Rectangle Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="7" Grid.RowSpan="1" Height="5" Fill="#FFE6E6E6" Width="auto" VerticalAlignment="Top" Margin="0,20,0,0"/>
                                <Image Grid.Column="1" Grid.Row="1" Height="125" Width="125" Source="Resources/Intervention-00001.png"/>
                                <CheckBox x:Name="Interventioncheck" Grid.Row="1" Grid.Column="1" VerticalAlignment="Bottom" HorizontalAlignment="Center" Checked="Interventioncheck_Checked" >Intervention</CheckBox>
                                <Image Grid.Column="2" Grid.Row="1" Height="125" Width="125" Source="Resources/Barrett50-00001.png"/>
                                <CheckBox x:Name="Barrett50check" Grid.Row="1" Grid.Column="2" VerticalAlignment="Bottom" HorizontalAlignment="Center" Checked="Barrett50check_Checked" >Barrett .50cal</CheckBox>
                                <Image Grid.Column="3" Grid.Row="1" Height="125" Width="125" Source="Resources/WA2000-00001.png"/>
                                <CheckBox x:Name="WA2000check" Grid.Row="1" Grid.Column="3" VerticalAlignment="Bottom" HorizontalAlignment="Center" Checked="WA2000check_Checked" >WA2000</CheckBox>
                                <Image Grid.Column="4" Grid.Row="1" Height="125" Width="125" Source="Resources/M14EBR-00001.png"/>
                                <CheckBox x:Name="M21EBRcheck" Grid.Row="1" Grid.Column="4" VerticalAlignment="Bottom" HorizontalAlignment="Center" Checked="M21EBRcheck_Checked" >M21 EBR</CheckBox>
                                <Image Grid.Column="5" Grid.Row="1" Height="125" Width="125" Source="Resources/M40A3-00001.png"/>
                                <CheckBox x:Name="M40A3check" Grid.Row="1" Grid.Column="5" VerticalAlignment="Bottom" HorizontalAlignment="Center" Checked="M40A3check_Checked" >M40A3</CheckBox>
                                <Image Grid.Column="1" Grid.Row="2" Height="125" Width="125" Source="Resources/Dragunov-00001.png"/>
                                <CheckBox x:Name="Dragunovcheck" Grid.Row="2" Grid.Column="1" VerticalAlignment="Bottom" HorizontalAlignment="Center" Checked="Dragunovcheck_Checked" >Dragunov</CheckBox>
                                <TextBlock Grid.Column="6" Grid.Row="1" Grid.RowSpan="4" Grid.ColumnSpan="2" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="#FF676464">Attachments Coming Soon</TextBlock>

                            </Grid>
                        </TabItem>
                        <TabItem Header="Shotguns">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="1"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="10"></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="10"></RowDefinition>
                                    <RowDefinition Height="*"></RowDefinition>
                                    <RowDefinition Height="*"></RowDefinition>
                                    <RowDefinition Height="*"></RowDefinition>
                                    <RowDefinition Height="*"></RowDefinition>
