                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="0"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="0.4*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="0"></RowDefinition>
                                    <RowDefinition Height="*"></RowDefinition>
                                    <RowDefinition Height="0.2*"></RowDefinition>
                                    <RowDefinition Height="0.2*"></RowDefinition>
                                    <RowDefinition Height="0.2*"></RowDefinition>
                                    <RowDefinition Height="*"></RowDefinition>
                                    <RowDefinition Height="0"></RowDefinition>
                                </Grid.RowDefinitions>
                                <Slider x:Name="dvarAnimSlider" Grid.Column="2" Grid.Row="3" Grid.ColumnSpan="2" VerticalAlignment="Center" ValueChanged="dvarAnimSlider_ValueChanged" MouseMove="dvarAnimSlider_MouseMove"></Slider>
                                <ui:NumberBox x:Name="fromAnimBtn" Grid.Column="1" Grid.Row="3" VerticalAlignment="Center" HorizontalAlignment="Right" Margin="0,0,15,0" Text="65" TextAlignment="Center"></ui:NumberBox>
                                <TextBox x:Name="selectedDvarAnim" Grid.Column="2" Grid.Row="2" Grid.ColumnSpan="2" VerticalAlignment="Center" HorizontalAlignment="Stretch" Margin="5,5,5,5">cg_fov</TextBox>
                                <ui:NumberBox x:Name="speedAnimBtn" Grid.Column="2" Grid.Row="4" VerticalAlignment="Center" HorizontalAlignment="Stretch" Margin="5,0,5,0" TextAlignment="Center" Text="0.5"/>
                                <Button x:Name="startAnimationBtn" Grid.Column="3" Grid.Row="4" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="5,10,5,10" Click="startAnimationBtn_Click">Start Animation</Button>
                                <ui:NumberBox x:Name="toAnimBtn" Grid.Column="4" Grid.Row="3" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="15,0,0,0" TextAlignment="Center" Text="110"></ui:NumberBox>
                            </Grid>
                        </TabItem>
                    </TabControl>
                </Grid>
            </TabItem>
            <TabItem x:Name="otherTab" Height="60" IsEnabled="False" >
                <TabItem.Header>
                    <StackPanel Orientation="Horizontal">
                        <Image Source="Resources/other.png" Width="22" Height="22" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="10,0,0,0" />
                    </StackPanel>
                </TabItem.Header>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="0"></ColumnDefinition>
                        <ColumnDefinition Width="*"></ColumnDefinition>
                        <ColumnDefinition Width="0"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="0"></RowDefinition>
                        <RowDefinition Height="*"></RowDefinition>
                        <RowDefinition Height="20"></RowDefinition>
                    </Grid.RowDefinitions>
                    <TabControl Grid.Row="1" Grid.Column="1">
                        <TabItem Header="Bone Cam">
                            <Grid x:Name="boneCamPresetBox">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="0"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="*"></ColumnDefinition>
                                    <ColumnDefinition Width="0"></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="0"></RowDefinition>
                                    <RowDefinition Height="50"></RowDefinition>
                                    <RowDefinition Height="50"></RowDefinition>
                                    <RowDefinition Height="0"></RowDefinition>
                                    <RowDefinition Height="*"></RowDefinition>
                                    <RowDefinition Height="*"></RowDefinition>
                                    <RowDefinition Height="*"></RowDefinition>
                                    <RowDefinition Height="*"></RowDefinition>
                                    <RowDefinition Height="*"></RowDefinition>
                                    <RowDefinition Height="0"></RowDefinition>
                                </Grid.RowDefinitions>
                                <CheckBox x:Name="enableBoneCheck" Grid.Column="1" Grid.Row="1" Grid.ColumnSpan="2" HorizontalAlignment="Center" Click="enableBoneCheck_Click" >Enable Bone Cam</CheckBox>
                                <ComboBox x:Name="selectBoneBox" Grid.Column="1" Grid.Row="2" Grid.ColumnSpan="2" HorizontalAlignment="Stretch" VerticalAlignment="Top" Margin="20,0,20,0" IsEnabled="False" SelectedIndex="1" SelectionChanged="selectBoneBox_SelectionChanged">
                                    <ListBoxItem Content="j_gun"/>
                                    <ListBoxItem Content="j_head"/>
                                    <ListBoxItem Content="tag_weapon_right"/>
                                    <ListBoxItem Content="tag_weapon_left"/>
                                </ComboBox>
                                <TextBlock Grid.Column="1" Grid.Row="4" HorizontalAlignment="Center">Position</TextBlock>
                                <TextBlock Grid.Column="2" Grid.Row="4" HorizontalAlignment="Center">Rotation</TextBlock>
                                <Slider x:Name="position1" Minimum="-180" Maximum="180" Grid.Column="1" Grid.Row="4" VerticalAlignment="Center" Margin="10,0,10,0" IsEnabled="False" ValueChanged="position1_ValueChanged"></Slider>
                                <TextBlock x:Name="position1Text" Grid.Column="1" Grid.Row="4" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,35">0</TextBlock>
                                <Slider x:Name="position2" Minimum="-180" Maximum="180" Grid.Column="1" Grid.Row="5" VerticalAlignment="Center" Margin="10,0,10,0" IsEnabled="False" ValueChanged="position2_ValueChanged"></Slider>
                                <TextBlock x:Name="position2Text" Grid.Column="1" Grid.Row="5" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,35">0</TextBlock>
                                <Slider x:Name="position3" Minimum="-180" Maximum="180" Grid.Column="1" Grid.Row="6" VerticalAlignment="Center" Margin="10,0,10,0" IsEnabled="False" ValueChanged="position3_ValueChanged"></Slider>
                                <TextBlock x:Name="position3Text" Grid.Column="1" Grid.Row="6" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,35">0</TextBlock>
                                <Slider x:Name="rotation1" Minimum="-180" Maximum="180" Grid.Column="2" Grid.Row="4" VerticalAlignment="Center" Margin="10,0,10,0" IsEnabled="False" ValueChanged="rotation1_ValueChanged"></Slider>
                                <TextBlock x:Name="rotation1Text" Grid.Column="2" Grid.Row="4" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,35">0</TextBlock>
                                <Slider x:Name="rotation2" Minimum="-180" Maximum="180" Grid.Column="2" Grid.Row="5" VerticalAlignment="Center" Margin="10,0,10,0" IsEnabled="False" ValueChanged="rotation2_ValueChanged"></Slider>
                                <TextBlock x:Name="rotation2Text" Grid.Column="2" Grid.Row="5" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,35">0</TextBlock>
                                <Slider x:Name="rotation3" Minimum="-180" Maximum="180" Grid.Column="2" Grid.Row="6" VerticalAlignment="Center" Margin="10,0,10,0" IsEnabled="False" ValueChanged="rotation3_ValueChanged"></Slider>
                                <TextBlock x:Name="rotation3Text" Grid.Column="2" Grid.Row="6" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,35">0</TextBlock>
                                <TextBlock Grid.Column="1" Grid.Row="7" Grid.ColumnSpan="2" HorizontalAlignment="Center">FOV</TextBlock>
                                <Slider x:Name="fovSlider" Minimum="0.2" Maximum="2" Grid.Column="1" Grid.Row="7" Grid.ColumnSpan="2" VerticalAlignment="Center" Margin="10,0,10,0" IsEnabled="False" ValueChanged="fovSlider_ValueChanged"></Slider>
                                <TextBlock Grid.Row="8" Grid.Column="1" Grid.ColumnSpan="2" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="12" >MW2 Bone Cam is not the greatest(quite buggy), you can only use it in a game as well.</TextBlock>
                                <ListBox x:Name="presetListBox" Grid.Row="1" Grid.RowSpan="8" Grid.Column="3" Margin="10,30,10,10" MouseDoubleClick="presetListBox_MouseDoubleClick">
                                    <ListBoxItem Content="Side Head"/>
                                    <ListBoxItem Content="Front Head"/>
                                    <ListBoxItem Content="Corner Gun"/>
                                    <ListBoxItem Content="Front Gun"/>
                                    <ListBoxItem Content="Top Head"/>
                                    <ListBoxItem Content="Back Head"/>
                                </ListBox>
                                <TextBlock Grid.Row="1" Grid.Column="3" HorizontalAlignment="Center" Margin="0,5,0,0">Presets</TextBlock>

                            </Grid>
                        </TabItem>
                        <TabItem Header="FX">

                        </TabItem>
                        <TabItem Header="Misc">

                        </TabItem>
                    </TabControl>
                </Grid>

            </TabItem>

        </TabControl>
        <TextBlock HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="5,0,0,253" Width="67" FontSize="9.5" >Selected Actor:</TextBlock>
        <ComboBox x:Name="ComboActorNum" VerticalAlignment="Bottom" Margin="5,0,0,232" Width="67" IsEnabled="False" FontSize="8.5" Height="22" SelectionChanged="ComboActorNum_SelectionChanged">
            <ComboBox.ContextMenu>
                <ContextMenu Name="cm" StaysOpen="true">
                    <MenuItem x:Name="artActorMenuBtn" Header="Open Artificial Actor Menu" Click="artActorMenuBtn_Click"></MenuItem>
                </ContextMenu>
            </ComboBox.ContextMenu>
        </ComboBox>
        <Button x:Name="createActorBtn" VerticalAlignment="Bottom" Margin="5,0,0,175" Height="55" Width="67" Content="Create" IsEnabled="False" Click="createActorBtn_Click" />
        <Button x:Name="deleteActorBtn" VerticalAlignment="Bottom" Margin="5,0,0,144" Width="67" Content="Delete" IsEnabled="False" Click="deleteActorBtn_Click" FontSize="12"/>
        <Button x:Name="sendAllActorBtn" VerticalAlignment="Bottom" Margin="5,0,0,112" Width="67" Content="Apply All" IsEnabled="False" FontSize="12" Click="sendAllActorBtn_Click"/>
        <Button x:Name="teleportActorBtn" VerticalAlignment="Bottom" Margin="5,0,0,53" Width="67" Content="Move" IsEnabled="False" Click="teleportActorBtn_click" FontSize="10"/>
        <Button x:Name="backActorBtn" VerticalAlignment="Bottom" Margin="5,0,0,24" Width="67" Content="Reset" IsEnabled="False" FontSize="10" Click="backActorBtn_Click"/>
        <TextBlock HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="26,0,0,90" Width="67" FontSize="9.5" Grid.ColumnSpan="2" >Auto Apply</TextBlock>
        <TextBlock x:Name="StatusText" VerticalAlignment="Bottom" Panel.ZIndex="1" Foreground="White" Margin="3,0,0,0" HorizontalAlignment="Left" Grid.ColumnSpan="2" MouseRightButtonDown="StatusText_MouseRightButtonDown">Status: Waiting For Game...</TextBlock>
        <TextBlock x:Name="precacheBox" VerticalAlignment="Bottom" Panel.ZIndex="1" Foreground="White" Margin="0,0,3,0" HorizontalAlignment="Right" Grid.ColumnSpan="2" MouseRightButtonDown="precacheBox_MouseRightButtonDown" >No Precache Selected</TextBlock>

        <Rectangle x:Name="statusBar" Height="20" Fill="DodgerBlue" Panel.ZIndex="0" VerticalAlignment="Bottom" Grid.ColumnSpan="2"></Rectangle>

        <CheckBox x:Name="autosendCheck" VerticalAlignment="Bottom" Margin="3,0,0,80" Content="" FontSize="10" IsEnabled="False" FontStyle="Normal" Grid.ColumnSpan="2"/>
    </Grid>
</Window>